{
  "name": "example-integration-test-by-playwrite",
  "description": "This repository is example of integration test for frontend by Playwrite",
  "author": "mryhryki",
  "license": "MIT",
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  },
  "scripts": {
    "build": "vite build",
    "dev:view": "vite --open",
    "dev:server": "nodemon --watch ./src/server/ --ext ts --exec 'esbuild --bundle --platform=node --target=node16 ./src/server/index.ts | node'",
    "test:server": "esbuild --bundle --platform=node --target=node16 ./src/server/test.ts | node",
    "lint": "mryhryki-lint -d ./src/",
    "start": "vite preview",
    "type": "tsc",
    "type:watch": "tsc --watch"
  },
  "dependencies": {
    "@mryhryki/lint": "^0.0.10",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "body-parser": "^1.20.0",
    "esbuild": "^0.15.5",
    "express": "^4.18.1",
    "node-fetch": "^3.2.10",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4",
    "vite": "^3.0.8"
  },
  "nodemonConfig": {
    "delay": 200
  }
}
